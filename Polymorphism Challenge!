class Vehicle:
    def move(self):
        return "Moving..."


class Car(Vehicle):
    def move(self):
        return "ğŸš— Driving on the road"


class Plane(Vehicle):
    def move(self):
        return "âœˆï¸ Flying in the sky"


class Boat(Vehicle):
    def move(self):
        return "â›µ Sailing on the water"


# Polymorphism in action
vehicles = [Car(), Plane(), Boat()]

for v in vehicles:
    print(v.move())
